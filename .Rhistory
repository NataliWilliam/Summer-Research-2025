# Function
tb_model <- function(t, state, parms) {
with(as.list(c(state, parms)), {
dS  <- (rho * N -
ql*t1 * S*I1/N -
ql*t2 * S*I2/N -
ql*t3 * S*I3/N -
ql*t4 * S*I4/N +
z1*phi1*I1 + z2*phi2*I2 + z3*phi3*I3 + z4*phi4*I4 +
(1 - l)*alpha*N -
mu0*S)
dE1 <- ((1 - p)*ql*t1 * S*I1/N -
vL * E1 +
(1 - y1)*(1 - z1)*phi1 * I1 +
l*alpha*(1 - r2 - r3 - r4)*N -
mu0*E1)
dI1 <- (p*ql*t1 * S*I1/N +
vL*E1 -
phi1*I1 -
mu0*I1 -
mu*I1)
dE2 <- ((1 - p)*ql*t2 * S*I2/N -
vL * E2 +
(1 - y2)*(1 - z2)*phi2 * I2 +
gamma*(1 - z1)*y1*phi1 * I1 +
l*alpha*r2*N -
mu0*E2)
dI2 <- (p*ql*t2 * S*I2/N +
vL*E2 -
phi2*I2 -
mu0*I2 -
mu*I2)
dE3 <- ((1 - p)*ql*t3 * S*I3/N -
vL * E3 +
(1 - y2)*(1 - z3)*phi3 * I3 +
(1 - gamma)*(1 - z1)*y1*phi1 * I1 +
l*alpha*r3*N -
mu0*E3)
dI3 <- (p*ql*t3 * S*I3/N +
vL*E3 -
phi3*I3 -
mu0*I3 -
mu*I3)
dE4 <- ((1 - p)*ql*t4 * S*I4/N -
vL * E4 +
(1 - z2)*y2*phi2 * I2 +
(1 - z3)*y2*phi3 * I3 +
(1 - z4)*phi4 * I4 +
l*alpha*r4*N -
mu0*E4)
dI4 <- (p*ql*t4 * S*I4/N +
vL*E4 -
phi4*I4 -
mu0*I4 -
mu*I4)
dD  <- (mu * (I1 + I2 + I3 + I4))
dN  <- (rho*N +
alpha*N -
mu*(I1 + I2 + I3 + I4) -
mu0*N)
list(c(dS, dE1, dI1, dE2, dI2, dE3, dI3, dE4, dI4, dD, dN))
})
}
